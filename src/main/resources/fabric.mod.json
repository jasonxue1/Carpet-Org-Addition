{
  "schemaVersion": 1,
  "id": "carpet-org-addition",
  "version": "${version}",
  "name": "Carpet Org Addition",
  "description": "一个非常Not Vanilla的Carpet扩展模组，提供了一些实用和无用的功能，\n默认情况下不会改变游戏的任何机制，所有功能都需要手动开启。",
  "authors": [
    "fcsailboat",
    "1024_byteeeee",
    "wendavid552",
    "liuyuexiaoyu1",
    "jasonxue1"
  ],
  "contact": {
    "sources": "https://github.com/fcsailboat/Carpet-Org-Addition",
    "issues": "https://github.com/fcsailboat/Carpet-Org-Addition/issues"
  },
  "license": "MIT",
  "icon": "assets/carpet-org-addition/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "boat.carpetorgaddition.generator.DataGenerator"
    ],
    "client": [
      "boat.carpetorgaddition.client.CarpetOrgAdditionClient"
    ],
    "main": [
      "boat.carpetorgaddition.CarpetOrgAddition"
    ]
  },
  "mixins": [
    "carpet-org-addition.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${depend_version}",
    "fabric-api": "*",
    "carpet": "*"
  },
  "accessWidener": "carpet-org-addition.accesswidener",
  "custom": {
    "buildTimestamp": "${buildTimestamp}"
  }
}
